{
  "name": "@twaice/nestjs-kinesis-producer",
  "version": "0.0.2",
  "description": "Kinesis Producer Library for NestJS",
  "author": "TWAICE <it@twaice.com>",
  "private": false,
  "repository": "https://github.com/TWAICE/nestjs-kinesis-producer",
  "license": "MIT",
  "main": "dist/index.js",
  "keywords": [
    "NestJS",
    "Kinesis",
    "Producer"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "version": "standard-release"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.5.1"
  },
  "dependencies": {
    "@twaice/node-kinesis-producer": "git+https://github.com/T31M/node-kinesis-producer.git#f4e89bac9536d78eb16302fe16dfd1cf82563b37"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.18",
    "@nestjs/schematics": "^7.3.1",
    "@types/node": "^14.17.6",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.2.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ]
  }
}
